<h1>Listing shipping_rates</h1>

<table>
  <tr>
  </tr>

<% @shipping_rates.each do |shipping_rate| %>
  <tr>
    <td><%= shipping_rate.id %></td>
    <td><%= shipping_rate.name %></td>
    <td><%= link_to 'Show', admin_shipping_rate_path(shipping_rate) %></td>
    <td><%= link_to 'Edit', edit_admin_shipping_rate_path(shipping_rate) %></td>
    <td><%= link_to 'Destroy', admin_shipping_rate_path(shipping_rate), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New shipping_rate', new_admin_shipping_rate_path %>